// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./database.sqlite"
}


model User {
  id        Int     @id @default(autoincrement())
  email     String  @unique
  username  String
  firstname String
  lastname  String
  password  String
}